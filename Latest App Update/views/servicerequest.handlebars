<p class="username">Hello {{username}}</p>
<p>Aptno: {{aptno}}</p>
<h1 class="cheaders"> Service Request </h1>
<p>{{error}}</p>
<section class="content">
    {{# each requests }}
            <article class="notes">
                <div>Aptno: {{ this.aptno }}</div>
                <div>Servicetype: {{ this.servicetype }}</div>
                <div>Message: {{ this.message }}</div>
                <div>Date: {{ this.date }}</div>
                <div>Id: {{this.id}}</div>
            </article>
    {{/each}}
</section>
<p><a href="/requests/addRequest">Add Request</a></p>
<p><a href="/requests/updateRequest">Update Request</a></p>
<p><a href="/requests/deleteRequest">Delete Request</a></p>